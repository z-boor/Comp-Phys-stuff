#define your function then solve   
def f(x):
    return function
def simpson (a, b, N):
    h = (b-a)/N
    I = 1/3*h*(f(a)+f(b))
    for k in range(1,N,2):
        I += 4/3*h*f(a+k*h)
    for k in range(2,N,2):
        I += 2/3*h*f(a+k*h)
    return I 

def df4(x):
  return function

def simpson_err(a,b,N)
I = 1/3*h*(f(a)+f(b))
h = (b-a)/N # height of function
